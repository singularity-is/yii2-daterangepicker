{
    "name": "singularity/yii2-daterangepicker",
    "description": "Date range picker for Yii2 framework",
    "keywords": [
        "singularity",
        "singularity solution",
        "yii",
        "yii2",
        "yii 2",
        "widget",
        "typeahead"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/singularity-is/yii2-daterangepicker",
    "authors": [
        {
            "name": "Singularity Solution",
            "email": "hello@singularity.is",
            "homepage": "https://singularity.is",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.28",
        "yiisoft/yii2-bootstrap4": "^2.0.8 ",
        "npm-asset/daterangepicker": "3.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "singularity\\daterangepicker\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
